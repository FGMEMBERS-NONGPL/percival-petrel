<?xml version="1.0"?>
  <!-- Oil temp indicator -->
  <PropertyList> 
    <path>oil-temp.ac</path>
    <animation> 
      <type>rotate</type>
      <object-name>needle</object-name>
      <property>engines/engine[0]/oil-temperature-degf</property>
      <interpolation> 
        <entry> 
          <ind>32</ind>
          <dep>-42.5</dep>
        </entry>
        <entry> 
          <ind>68</ind>
          <dep>-28</dep>
        </entry>
        <entry> 
          <ind>104</ind>
          <dep>-14</dep>
        </entry>
        <entry> 
          <ind>140</ind>
          <dep>0</dep>
        </entry>
        <entry> 
          <ind>176</ind>
          <dep>14</dep>
        </entry>
        <entry> 
          <ind>212</ind>
          <dep>28</dep>
        </entry>
        <entry> 
          <ind>248</ind>
          <dep>42.5</dep>
        </entry>
      </interpolation>
      <center>
        <x-m>0.0</x-m>
        <y-m>0.0</y-m>
        <z-m>-0.0214</z-m>
      </center>
      <axis> 
        <x>-1</x>
        <y>0</y>
        <z>0</z>
      </axis>
    </animation>

    <animation> 
      <type>material</type>
      <emission> 
        <factor-prop>controls/lighting/panel-norm</factor-prop>
        <red>0.7</red>
        <green>0.7</green>
        <blue>0.7</blue>
      </emission>
    </animation>

    <animation>
	<type>noshadow</type>
    </animation>

    <!-- *** LoD *** -->
    <animation> 
      <type>range</type>
      <min-m>0</min-m>
      <max-m>6</max-m>
    </animation>
  </PropertyList>
